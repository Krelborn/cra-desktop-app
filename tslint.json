{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "double"
    ]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
